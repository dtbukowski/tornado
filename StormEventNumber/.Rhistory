get_storm_event_table <- function (filename) {
dat <- read.csv(filename,stringsAsFactors=FALSE)
dat <- transform(dat,
lat  = round(LATITUDE),
long = round(LONGITUDE))
dat <- transform(dat,
lat  = factor(lat,-14:63),
long = factor(long,-171:-65))
return(table(dat$lat,dat$long))
}
